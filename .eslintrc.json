{
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "extends": ["airbnb-base"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "use": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "class-methods-use-this": ["off"],
      "strict": ["off"]
    }
  }